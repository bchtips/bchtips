{
	"name": "BCH Tips",
	"description": "Send Bitcoin Cash tips on social media easily and securely.",
	"manifest_version": 2,
	"version": "1.0",
	"key": "",
	"icons": {
		"128": "img/icon.png"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"storage",
		"tabs",
		"notifications",
		"https://*.reddit.com/*",
		"https://twitter.com/*"
		],
	"content_scripts": [{
	"matches": ["https://*.reddit.com/*"],
	"css": ["reddit.css"],
	"js": ["lib/bchaddrjs-0.2.0.min.js","lib/bitcoincash-0.1.10.min.js","lib/chrome-Storage-largeSync.min.js","lib/lz-string.min.js","reddit.js"]
	}],
	"web_accessible_resources": ["tx.html"]
}
